{
  "monday": {
    "id": "monday-density-strength",
    "name": "Density Strength",
    "focus": "strength",
    "day": "monday",
    "goal": "Build wire-strong muscle density. Stop sets 1 rep before failure.",
    "warmup": {
      "name": "The Armor Warm-up",
      "duration": 8,
      "exercises": [
        {
          "id": "warmup-1",
          "name": "5 min Rower or Elliptical",
          "duration": 300,
          "notes": "Light sweat"
        },
        {
          "id": "warmup-2",
          "name": "Finger Extensions",
          "sets": 2,
          "reps": 25,
          "notes": "Use rubber band; open hand wide. Crucial for elbow recovery."
        },
        {
          "id": "warmup-3",
          "name": "Scapular Pull-ups",
          "sets": 2,
          "reps": 10,
          "notes": "Hang straight, shrug shoulders down. SKIP if painful; do Wall Slides instead."
        },
        {
          "id": "warmup-4",
          "name": "Warm-up Sets",
          "notes": "1x15 Straight-Arm Pulldowns (Light), 1x5 Light Press"
        }
      ]
    },
    "exercises": [
      {
        "id": "monday-a1",
        "name": "Weighted Neutral-Grip Pull-ups",
        "sets": 3,
        "reps": 5,
        "rest": 120,
        "isStandard": true,
        "form": "Standard pull-up form",
        "alternative": {
          "name": "Straight-Arm Cable Pulldowns",
          "sets": 3,
          "reps": 15,
          "form": "Keep arms locked straight. Push bar to thighs using lats. Zero elbow bending."
        },
        "isElbowSafe": false
      },
      {
        "id": "monday-a2",
        "name": "DB Push Press",
        "sets": 3,
        "reps": 6,
        "rest": 120,
        "form": "Slight leg dip to drive up, 3-sec slow lower."
      },
      {
        "id": "monday-b1",
        "name": "Romanian Deadlift (RDL)",
        "sets": 3,
        "reps": 6,
        "rest": 90,
        "form": "Hips back. Use Straps to protect your elbow."
      },
      {
        "id": "monday-b2",
        "name": "DB Reverse Lunges",
        "sets": 3,
        "reps": 10,
        "rest": 90,
        "form": "Step back far. Drive through the front heel.",
        "notes": "8-10 reps per leg"
      },
      {
        "id": "monday-c",
        "name": "Heavy Farmer's Carry",
        "isStandard": true,
        "alternative": {
          "name": "Bear Hug Carry",
          "sets": 3,
          "form": "Hug a heavy plate or sandbag to your chest. Squeeze tight with arms, not hands."
        },
        "notes": "Max Time",
        "isElbowSafe": false
      },
      {
        "id": "monday-d",
        "name": "Cable Pull-Throughs",
        "sets": 3,
        "reps": 20,
        "form": "Face away from machine. Rope between legs. Hinge hips back, snap forward."
      }
    ],
    "notes": [
      "Add 1/2 tsp salt to your water for every workout",
      "Stop sets 1 rep before failure"
    ]
  },
