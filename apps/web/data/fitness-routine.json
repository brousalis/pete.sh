  "dailyRoutines": {
    "night": {
      "id": "release-the-brakes",
      "name": "Night Stretch",
      "type": "night",
      "duration": 5,
      "exercises": [
        {
          "why": "Fixes tight hip flexors from running and hiking",
          "name": "Couch Stretch",
          "action": "Kneel in front of a couch (or wall). Put one shin vertical against the couch cushion behind you. Squeeze your glute and stand tall. You should feel a massive pull in the front of your hip/thigh. 60 seconds per side.",
          "duration": 120,
          "description": "The #1 stretch for you. Running and hiking tighten the hip flexors. Tight hip flexors cause lower back pain and \"weak\" glutes."
        },
        {
          "why": "Opens chest for better breathing and posture",
          "name": "Doorway Pec Stretch",
          "action": "Put your forearms against a doorframe at 90 degrees. Step through gently until you feel a stretch across your chest.",
          "duration": 60,
          "description": "Climbers and heavy lifters tend to roll their shoulders forward. This opens the chest so you can breathe better and stand taller."
        },
        {
          "why": "Loosens lats and lower back",
          "name": "Child's Pose with Side Reach",
          "action": "Kneel, sit back on your heels, and reach arms far forward on the floor. Then, walk both hands to the left (stretches right lat), hold, then walk to the right.",
          "duration": 120,
          "description": "Loosens the lats (from all the pull-ups) and the lower back (from the RDLs and hiking)."
        }
      ],
      "description": "Do this 30 minutes before bed. The goal is to lengthen the muscles that tightened up during your run or lift."
    },
    "morning": {
      "id": "oil-the-hinge",
      "name": "Morning Stretch",
      "type": "morning",
      "duration": 3,
      "exercises": [
        {
          "why": "Opens ankles, knees, and hips for the day",
          "name": "Deep Squat Hold",
          "action": "Drop into the deepest squat you can. Keep heels flat. Use your elbows to push your knees out. Rock side to side gently.",
          "duration": 60,
          "description": "Opens ankles, knees, and hips for the day. Essential for the \"high step\" moves in climbing."
        },
        {
          "why": "Prevents hunchback posture from pulling and carrying",
          "name": "T-Spine Rotation",
          "action": "On hands and knees, put one hand behind your head. Rotate that elbow down to touch the opposite wrist, then rotate it all the way up to the ceiling. 30 seconds per side.",
          "duration": 60,
          "description": "Prevents developing a \"hunchback\" posture from all the pulling and carrying."
        },
        {
          "why": "Decompresses spine and wakes up grip",
          "name": "Dead Hang",
          "action": "Just hang from a bar (or doorframe if sturdy). Let your back crack and pop.",
          "duration": 60,
          "description": "Decompresses your spine and wakes up your grip."
        }
      ],
      "description": "Do this right after you wake up. The goal is to open up your hips and upper back, which get stiff from sleeping."
    }
  },
